<h2>Listing answers</h2>

<table>
  <% @answers.each do |answer| %>
      <tr>
        <td><%=h answer.id %></td>
        <td><%=h answer.user_id %></td>
        <td><%=h answer.question_id %></td>
        <td><%=h answer.answer %></td>
        <td><%= link_to "Show", answer %></td>
      </tr>
  <% end %>
</table>

<br />

<%= link_to 'New answer', new_answer_path %>
<%= link_to 'Back', account_path %>